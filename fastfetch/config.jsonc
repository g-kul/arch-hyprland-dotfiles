{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        //"type": "file",
        //"source": "~/.config/fastfetch/cat.txt",
        //"color": {
        //    "1": "cyan",
        //    "2": "red",
        //    "3": "yellow",
        //    "4": "blue",
        //    "5": "green"
        //}
       // "source": "~/.config/fastfetch/archpuccinn.png",│
        "source": "~/.config/newlogo.png",
        "type": "kitty",
        "height": 19,
        "width": 52,
        "padding": {
            "top": 0,
           "left": 0,
          "right": 0
        }
    },
    "display": {
      "separator": " ",     // Separator between keys and values
      "color": {
         "keys": "#baffc9",    // Key color
         "output": "#ffffba",
         "separator": "#ffffe3"// Title color
      },
    },
    "modules": [
        
        {
            "type": "custom",
            "format": " ",
            "outputColor": "yellow"
        },
        {
            "type": "custom",
            "format": " ",
            "outputColor": "yellow"
        },
        {
            "type": "custom",
            "format": "┌─────────────────────────────────────────────┐",
            "outputColor": "#ffffe3"
        },
        {
            "type": "os",
            "key": " 💻 OS          ",
            //"keyColor": "yellow"
        },
        {
            "type": "kernel",
            "key": " 🧬 Kernel      ",
            //"keyColor": "yellow"
        },
        {
            "type": "memory",
            "key": " 🧠 Ram         ",
            "format": "{1} / {2}",
            //"keyColor": "yellow"
        }, 
        {
            "type": "packages",
            "key": " 📦 Packages    ",
            //"keyColor": "yellow"
        },
        {
            "type": "shell",
            "key": " 🔧 Shell       ",
            //"keyColor": "yellow"
        },
        {
            "type": "command",
            "key": " 📅 OS Age      ",
            //"keyColor": "yellow",
            "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
        },
        {
            "type": "uptime",
            "key": " ⏱️ Uptime      ",
            //"keyColor": "yellow"
        },
        {
            "type": "gpu",
            "key": " 🎮 GPU Driver  ",
            "format": "{3}",
            //"keyColor": "yellow"
        },
        { 
            "type": "battery",
            "key": " 🔋 Battery     ",
            "format": "{4} [{6} {5} ]",
            //"keyColor": "yellow"
        },
        {
            "type": "wmtheme",
            "key": " 󰉼 Theme         ",
            //"keyColor": "yellow"
        },
        {
            "type": "wifi",
            "key": " 📡 WiFi        ",
            "format": "{4} [{13}GHz]",
            //"keyColor": "yellow"
        },
        {
            "type": "custom",
            "format": "└─────────────────────────────────────────────┘",
            "outputColor": "#ffffe3"
        },
        {
            "type": "custom",
            "format": " ",
            "outputColor": "yellow"
        }
    ]
}
